{
	"_version": "1.7.0",
	"sap.app": {
		"id": "halo.sap.mm.RECIPECOST",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZRECIPECOST_ODATA_SRV": {
				"uri": "/sap/opu/odata/sap/ZRECIPECOST_ODATA_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"mainService": {
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				},
				"uri": "/sap/opu/odata/sap/ZRECIPECOST_ODATA_SRV/"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "halo.sap.mm.RECIPECOST.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "halo.sap.mm.RECIPECOST.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ZRECIPECOST_ODATA_SRV"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "halo.sap.mm.RECIPECOST.pages.view",
				"controlId": "idApp",
				"controlAggregation": "pages",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "default",
					"target": [
						"recipehome"
					]
				},
				
				{
					"pattern": "recipehome",
					"name": "recipehome",
					"target": "recipehome"
				},
				{
					"pattern": "recipes/{Ekorg}/{Werks}/{GroupID}",
					"name": "recipes",
					"target": "recipes"
				},
				{
					"pattern": "imageeditor",
					"name": "imageeditor",
					"target": "imageeditor"
				},
				{
					"pattern": "recipeloc/{Werks}",
					"name": "recipeloc",
					"target": "recipeloc"
				},
				{
					"pattern": "recipegroup/{Werks}",
					"name": "recipegroup",
					"target": "recipegroup"
				},
				{
					"pattern": "recipegrouptiles/{Ekorg}/{Werks}",
					"name": "recipegrouptiles",
					"target": "recipegrouptiles"
				},
				{
					"pattern": "recipeoverview/{Ekorg}/{Werks}/{RecipeID}",
					"name": "recipeoverview",
					"target": "recipeoverview"
				},
				{
					"pattern": "uomconversion/{Ekorg}/{Werks}",
					"name": "uomconversion",
					"target": "uomconversion"
				},
				{
					"pattern": "planttopurchorg",
					"name": "planttopurchorg",
					"target": "planttopurchorg"
				},
				{
					"pattern": "rptrange",
					"name": "rptrange",
					"target": "rptrange"
				},
				{
					"pattern": "rptoutbartable/{Ekorg}/{Werks}/{FilterType}/{P1}/{P2}",
					"name": "rptoutbartable",
					"target": "rptoutbartable"
				},
				{
					"pattern": "ingredientform/{Ekorg}/{Werks}/{RecipeID}",
					"name": "ingredientform",
					"target": "ingredientForm"
				}
			],
			"targets": {
				"masterdetail": {
					"viewName": "MasterDetail",
					"viewLevel": 1,
					"controlId": "idApp",
					"controlAggregation": "pages",
					"viewId": "masterdetail"
				},
				"recipehome": {
					"parent": "masterdetail",
					"viewId": "recipehome",
					"viewName": "RecipeHome",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"recipes": {
					"parent": "masterdetail",
					"viewId": "recipes",
					"viewName": "Recipes",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"imageeditor": {
					"parent": "masterdetail",
					"viewId": "imageeditor",
					"viewName": "ImageEditor",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"rptrange": {
					"parent": "masterdetail",
					"viewId": "rptrange",
					"viewName": "RptRange",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"rptoutbartable": {
					"viewId": "rptoutbartable",
					"viewName": "RptOutBarTable",
					"viewLevel": 1
				},
				"recipeloc": {
					"parent": "masterdetail",
					"viewId": "recipieloc",
					"viewName": "RecipeLocation",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"recipegroup": {
					"parent": "masterdetail",
					"viewId": "recipiegroup",
					"viewName": "RecipeGroup",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"recipegrouptiles": {
					"parent": "masterdetail",
					"viewId": "recipiegrouptiles",
					"viewName": "RecipeGroupTiles",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"recipeoverview": {
					"viewId": "recipeoverview",
					"viewName": "RecipeOverview",
					"viewLevel": 1
				},
				"uomconversion": {
					"parent": "masterdetail",
					"viewId": "uomconversion",
					"viewName": "UOMConversion",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"planttopurchorg": {
					"parent": "masterdetail",
					"viewId": "planttopurchorg",
					"viewName": "PlantToPurchOrg",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"ingredientForm": {
					"parent": "masterdetail",
					"viewId": "ingredientForm",
					"viewName": "IngredientForm",
					"viewLevel": 1,
					"controlId": "app",
					"controlAggregation": "pages"
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zrecipecost/webapp",
		"_version": "1.1.0"
	}
}