{
	"_version": "1.7.0",
	"sap.app": {
		"id": "halo.sap.mm.RECIPECOST",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZRECIPECOST_ODATA_SRV": {
				"uri": "/sap/opu/odata/sap/ZRECIPECOST_ODATA_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "halo.sap.mm.RECIPECOST.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "halo.sap.mm.RECIPECOST.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ZRECIPECOST_ODATA_SRV"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "halo.sap.mm.RECIPECOST.pages.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "recipes",
					"name": "recipes",
					"target": "recipes"
				},
				{
					"pattern": "recipeloc",
					"name": "recipeloc",
					"target": "recipeloc"
				},
				{
					"pattern": "ingredientform",
					"name": "ingredientform",
					"target": "ingredientForm"
				}
			],
			"targets": {
				"recipes": {
					"viewId": "recipes",
					"viewName": "Recipes",
					"viewLevel": 1
				},
				"recipeloc": {
					"viewId": "recipieloc",
					"viewName": "RecipeLocation",
					"viewLevel": 1
				},
				"ingredientForm": {
					"viewId": "ingredientForm",
					"viewName": "IngredientForm",
					"viewLevel": 1
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}